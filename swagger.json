              "schema": {
                "type": "object",
                "properties": {
                  "Role": {
                    "type": "string",
                    "pattern": "^[A-Za-z0-9]+$",
                    "example": "L2"
                  },
                  "Fullname": {
                    "type": "string",
                    "pattern": "^[A-Za-z0-9 ]+$",
                    "example": "Said Santana"
                  },
                  "Email": {
                    "type": "string",
                    "pattern": "^([A-Za-z0-9_\\-\\.\\+])+\\@(([a-zA-Z0-9\\-])+\\.)+([a-zA-Z0-9]{2,4})+$",
                    "example": "said.santana@hp.com"
                  },
                  "MGREmail": {
                    "type": "string",
                    "pattern": "^([A-Za-z0-9_\\-\\.\\+])+\\@(([a-zA-Z0-9\\-])+\\.)+([a-zA-Z0-9]{2,4})+$",
                    "example": "albert.fan@hp.com"
                  },
                  "UpdatedBy": {
                    "type": "string",
                    "pattern": "^[A-Za-z0-9 ]+$",
                    "nullable": true,
                    "example": "SaSa"
                  }
                },
                "required": [
                  "Role",
                  "Fullname",
                  "Email",
                  "MGREmail"
                ],
                "additionalProperties": false
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": {
                  "Endpoint": "put /updateUsers",
                  "Timestamp": "2024-07-12T06:35:46.507Z",
                  "Message": "[/updateUsers] process successfully",
                  "Result": {}
                }
              }
            }
          },
          "500": {
            "description": "Error response",
            "content": {
              "application/json": {
                "example": {
                  "Timestamp": "2024-07-11T06:17:12.008Z",
                  "ErrorCode": "90028",
                  "Endpoint": "put /updateUsers",
                  "Suggestion": "Please provide a/an object input.",
                  "Message": "INVALID_PARAMETER_TYPE - [example error message]"
                }
              }
            }
